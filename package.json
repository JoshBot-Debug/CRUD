{
  "name": "total-accounting",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "npx prettier . -w",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=drizzle.config.ts",
    "db:export": "drizzle-kit export --config=drizzle.config.ts",
    "db:check": "drizzle-kit check --config=drizzle.config.ts",
    "db:up": "drizzle-kit up --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "seed": "npx tsx app/.server/api/db/seed.ts seed"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-charts": "^8.14.0",
    "@mui/x-data-grid": "^8.14.0",
    "@mui/x-date-pickers": "^8.14.0",
    "@mui/x-tree-view": "^8.14.0",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "autosuggest-highlight": "^3.3.4",
    "commander": "^14.0.2",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "downloadjs": "^1.4.7",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "http-problem-details": "^0.1.7",
    "isbot": "^5.1.31",
    "lottie-react": "^2.4.1",
    "nprogress": "^0.2.0",
    "pg": "^8.16.3",
    "pluralize": "^8.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-number-format": "^5.4.4",
    "react-router": "^7.9.2",
    "to-words": "^4.8.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/downloadjs": "^1.4.6",
    "@types/node": "^22",
    "@types/nprogress": "^0.2.3",
    "@types/pg": "^8.15.6",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "drizzle-kit": "^0.31.7",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
